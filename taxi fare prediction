      "source": [
        "#Apply model on test data\n",
        "predictions_DT = fit_DT.predict(test1.iloc[:,1:7])\n",
        "#Calculate MAPE\n",
        "def MAPE(y_true, y_pred):\n",
        "    mape = np.mean(np.abs((y_true - y_pred) / y_true))*100\n",
        "    return mape\n",
        "MAPE(test1.iloc[:,0], predictions_DT)"
      ],
      "metadata": {
